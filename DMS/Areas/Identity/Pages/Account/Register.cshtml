<body>
    <!-- Content -->
    <div class="container-xxl">
        <div class="authentication-wrapper authentication-basic container-p-y">
            <div class="authentication-inner">
                <!-- Register -->
                <div class="card">
                    <div class="card-body">
                        <!-- Logo -->
                        <div class="app-brand justify-content-center">
                            <a href="@Url.Action("Index","Home")" class="app-brand-link gap-2">
                                <img width="220" height="180" src="~/Images/logo-removebg.png" />
                            </a>
                        </div>
                        @*  <div class="app-brand justify-content-center">
                            <a href="index.html">
                                <span class="fw-bolder" style="font-size:30px;">DMS </span>
                            </a>
                        </div> *@
                        <!-- /Logo -->
                        <h4 class="mb-2" style="text-align:center;">Sign Up </h4>
                        @* <p class="mb-4">Please sign-in to your account and start the adventure</p> *@

                        <form id="account" method="post" class="mb-3">
                            <div asp-validation-summary="ModelOnly" class="text-danger" role="alert"></div>

                            <div class="mb-3">
                                <label asp-for="Input.Name" class="form-label">Name</label>
                                <input type="text" asp-for="Input.Name" class="form-control" placeholder="Last Name" autofocus>
                                <span asp-validation-for="Input.Name" class="text-danger"></span>
                            </div>


                            <div class="mb-3">
                                <label asp-for="Input.PhoneNumber" class="form-label">Phone Number</label>
                                <input type="text" asp-for="Input.PhoneNumber" class="form-control" placeholder="Phone or Cell" autofocus>
                                <span asp-validation-for="Input.PhoneNumber" class="text-danger"></span>
                            </div>


                            <div class="mb-3">
                                <label asp-for="Input.Email" class="form-label">Email </label>
                                <input type="text" asp-for="Input.Email" class="form-control" placeholder="Enter your email or username" autofocus>
                                <span asp-validation-for="Input.Email" class="text-danger"></span>
                            </div>
                            <div class="mb-3 form-password-toggle">
                                <label asp-for="Input.Password" class="form-label" for="password">Password</label>
                                <div class="input-group input-group-merge">

                                    <input asp-for="Input.Password" class="form-control" autocomplete="current-password" aria-required="true" placeholder="Password" />
                                    @* <input asp-for="Input.Password" type="password" id="password" class="form-control" name="password" placeholder="&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;" aria-describedby="password" /> *@
                                    <span asp-for="Input.Password" class="input-group-text cursor-pointer"><i class="bx bx-hide"></i></span>
                                    <span asp-validation-for="Input.Password" class="text-danger"></span>
                                </div>
                            </div>

                            <div class="mb-3 form-password-toggle">
                                <label asp-for="Input.ConfirmPassword" class="form-label" for="Confirm Password">Confirm Password</label>
                                <div class="input-group input-group-merge">

                                    <input asp-for="Input.ConfirmPassword" class="form-control" autocomplete="current-password" aria-required="true" placeholder="Confirm Password" />
                                    @* <input asp-for="Input.Password" type="password" id="password" class="form-control" name="password" placeholder="&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;&#xb7;" aria-describedby="password" /> *@
                                    <span asp-for="Input.ConfirmPassword" class="input-group-text cursor-pointer"><i class="bx bx-hide"></i></span>
                                    <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="mb-3">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" id="terms-conditions" name="terms">
                                    <label class="form-check-label" for="terms-conditions">
                                        I agree to
                                        <a href="javascript:void(0);">privacy policy & terms</a>
                                    </label>
                                </div>
                            </div>
                            <div class="mb-3">
                                <button id="login-submit" type="submit" class="btn btn-primary d-grid w-100" type="submit">Sign Up</button>
                            </div>
                        </form>
                        <p class="text-center">
                            <span>Already have an account?</span>
                            <a asp-page="./Login" asp-route-returnUrl="@Model.ReturnUrl">
                                <span>Sign in instead</span>
                            </a>
                        </p>

                        <div class="divider my-4">
                            <div class="divider-text">or</div>
                        </div>

                        <div class="d-flex justify-content-center">
                            <a href="javascript:;" class="btn btn-icon btn-label-facebook me-3">
                                <i class="tf-icons bx bxl-facebook"></i>
                            </a>

                            <a href="javascript:;" class="btn btn-icon btn-label-google-plus me-3">
                                <i class="tf-icons bx bxl-google-plus"></i>
                            </a>
                            @* 
                            <a href="javascript:;" class="btn btn-icon btn-label-twitter">
                                <i class="tf-icons bx bxl-twitter"></i>
                            </a> *@
                        </div>
                    </div>
                </div>
                <!-- /Register -->
